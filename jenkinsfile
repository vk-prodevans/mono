pipeline {
        agent none
        stages {
         
          stage("build & SonarQube Scanner") {
            agent any
 tools {
               jdk 'jdk17'
                maven 'maven3'
            }
            steps {
              withSonarQubeEnv('Sonar_pipeline') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
